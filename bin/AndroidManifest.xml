<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.plotadordehoras"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:targetPackage="com.example.plotadordehoras" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher" >
        <uses-library android:name="android.test.runner" />

        <activity
            android:name="com.example.plotadordehoras.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.plotadordehoras.activity.CadastroDeAtividade"
            android:label="@string/tela_cadastro_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
        <activity
            android:name="com.example.plotadordehoras.activity.CadastrarMinSoPraTeste"
            android:label="@string/title_activity_cadastrar_min_so_pra_teste" >
        </activity>
        <activity
            android:name="com.example.plotadordehoras.activity.AtividadesDaSemana"
            android:label="@string/title_activity_relatorio_semana" >
        </activity>
        <activity
            android:name="com.example.plotadordehoras.activity.RelatorioDaSemana"
            android:label="@string/title_activity_relatorio_da_semana" >
        </activity>
        <activity
            android:name="com.example.plotadordehoras.activity.Historico"
            android:label="@string/title_activity_historico" >
        </activity>
    </application>

</manifest>